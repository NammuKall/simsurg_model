# Weights & Biases Configuration
# Copy this file to .env and add your actual API token
WANDB_API_KEY=your_wandb_api_key_here

# Optional: Project and entity settings
WANDB_PROJECT=SimSurgModel
WANDB_ENTITY=nammukallur

# Training Configuration
BATCH_SIZE=2
LEARNING_RATE=0.001
NUM_EPOCHS=50
DEVICE=auto
NUM_WORKERS=2

# YOLOv5 Optimization Hyperparameters
WEIGHT_DECAY=0.0005
EARLY_STOPPING_PATIENCE=10
EARLY_STOPPING_MIN_DELTA=0.001

# Model Configuration
MODEL_NAME=EfficientDet
# Options: EfficientDet, ResNet, FasterRCNN, YOLOv5
# MODEL_VARIANT=EfficientDet-Small  # Optional: specify model variant
# YOLOv5 variants: YOLOv5-Nano, YOLOv5-Small, YOLOv5-Medium, YOLOv5-Large, YOLOv5-XLarge

# Data Paths (adjust as needed)
DATA_DIR=data/simsurgskill_2021_dataset
COCO_DIR=data/coco_format
RESULTS_DIR=results
MODEL_SAVE_DIR=models
